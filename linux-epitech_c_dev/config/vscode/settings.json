{
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "window.zoomLevel": 0,
    "epitech-c-cpp-headers.username": "francois.lefevre@epitech.eu",
    "epitech-c-cpp-headers.login": "francois.lefevre@epitech.eu",
    "epitech-c-cpp-headers.usePragmaOnce": false,
    "epitech-c-cpp-headers.autoGenerateClasses": false,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "workbench.colorTheme": "Solarized Dark",
    "editor.rulers": [
        80
    ],
    "files.autoSave": "off",
    "editor.renderWhitespace": "boundary",
    "editor.cursorStyle": "block-outline",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.formatOnPaste": true,
    "editor.smoothScrolling": true,
    "C_Cpp.loggingLevel": "Warning",
    "python.autoComplete.addBrackets": true,
    "C_Cpp.formatting": "Disabled",
    "workbench.startupEditor": "readme",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.a": true,
        "**/*.gcda": true,
        "**/*.gcno": true,
        "**/*.o": true,
        "**/*.out": true,
        "**/unit_tests": true,
        "**/vgcore*": true,
        "**/#*#": true
    },
    "explorer.confirmDelete": false,
    "C_Cpp.default.includePath": [
        "${default}",
        "${workspaceFolder}/include"
    ],
    "explorer.confirmDragAndDrop": false,
    "breadcrumbs.enabled": true,
    "cSpell.userWords": [
        "c'est",
        "csfml",
        "est",
        "explicite",
        "fichier",
        "hovr",
        "info",
        "jouer",
        "libmy",
        "libpng",
        "name",
        "parce",
        "parse",
        "play",
        "product",
        "scraped",
        "scrapy",
        "seul",
        "suis",
        "t",
        "xpath"
    ],
    "cSpell.language": "en,en-GB,en-US",
    "window.titleBarStyle": "custom"
}